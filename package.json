{
  "name": "compass-calendar",
  "description": "calendar monorepo root",
  "author": "Switchback Tech LLC <hello@switchback.tech>",
  "version": "1.0.0",
  "license": "SEE LICENSE IN LICENSE",
  "private": true,
  "browserslist": "last 2 Chrome versions",
  "main": "",
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "scripts": {
    "build": "rm -rf build/ && yarn tsc --build --clean && yarn build:backend && yarn build:web",
    "build:backend": "yarn tsc --build --verbose && yarn zip:backend",
    "build:web": "cd packages/web && webpack --mode=production --env production && yarn zip:web",
    "build:web -analyze": "cd packages/web && webpack --mode=production --env production analyze",
    "dev:desktop": "cross-env NODE_ENV=development electron .",
    "dev:backend": "export DEBUG=app* && export NODE_ENV=development && yarn dev:backend-start",
    "dev:backend -verbose": "export DEBUG=* && yarn dev:backend-start",
    "dev:backend-start": "cd packages/backend && ts-node-dev --respawn --pretty --transpile-only -r tsconfig-paths/register src/app.ts",
    "dev:web": "cd packages/web && webpack serve --env development --mode=development",
    "deploy:backend": "gcloud compute scp build/backend.zip ***REMOVED***@compass-backend1:/home/***REMOVED***/",
    "deploy:web": "gcloud compute scp build/web.zip ***REMOVED***@compass-backend1:/home/***REMOVED***/",
    "deploy": "yarn deploy:web & yarn deploy:backend",
    "lint -f": "cross-env yarn workspaces run lint-fix",
    "lint:css": "stylelint 'packages/web/src/**/*.ts'",
    "start:backend": "cp packages/backend/.env build/backend && export DEBUG=app* && export NODE_ENV=production && cd build/backend && node src/app.js",
    "start:desktop": "yarn prod:build && electron .",
    "start:web": "http-server build/web/",
    "tsc:backend": "rm -rf packages/backend/build && yarn tsc --project packages/backend/tsconfig.json",
    "tsc:core": "rm -rf packages/core/build && yarn tsc --project packages/core/tsconfig.json",
    "test": "yarn jest",
    "test:backend": "yarn jest backend",
    "test:core": "yarn test core",
    "test:web": "yarn test web",
    "zip": "yarn zip:backend && yarn zip:web",
    "zip:backend": "zip -r build/backend.zip build/backend build/core",
    "zip:web": "zip -r build/web.zip build/web",
    "_audit:desktop": "cd packages/electron && yarn electronegativity -i ./ -x LimitNavigationGlobalCheck,PermissionRequestHandlerGlobalCheck",
    "_db:clean": "node scripts/cli.js --delete",
    "_build:desktop": " cd packages/electron && electron-builder --dir",
    "_build:desktop -a": "cd packages/electron && electron-builder --mac --linux --windows",
    "_translate:desktop": "node packages/electron/localization/translateMissing.js"
  },
  "dependencies": {
    "@compass/backend": "*",
    "@compass/core": "*",
    "@compass/web": "*",
    "@types/uuid": "^8.3.1",
    "focus-trap-react": "^8.7.0",
    "inquirer": "^8.1.2",
    "jsonwebtoken": "^8.5.1",
    "lerna": "^4.0.0",
    "lodash": "4.17.21",
    "lodash.merge": "^4.6.2",
    "module-alias": "^2.2.2",
    "process": "^0.11.10",
    "source-map-support": "^0.5.20",
    "tslib": "^2.3.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "babel-loader": "^8.2.2",
    "buffer": "^6.0.3",
    "commander": "^8.1.0",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-lerna": "^2.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-testing-library": "^5.0.5",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "lint-staged": "^12.1.5",
    "path-browserify": "^1.0.1",
    "prettier": "^2.5.1",
    "stream-browserify": "^3.0.0",
    "ts-jest": "^27.1.2",
    "typescript": "^4.4.3"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": []
  }
}
