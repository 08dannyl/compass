{
  "name": "compass-calendar",
  "description": "calendar monorepo root",
  "author": "Switchback Tech LLC <hello@switchback.tech>",
  "version": "2.0.0",
  "license": "SEE LICENSE IN LICENSE",
  "private": true,
  "browserslist": "last 2 Chrome versions",
  "main": "",
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "scripts": {
    "audit:desktop": "cd packages/electron && yarn electronegativity -i ./ -x LimitNavigationGlobalCheck,PermissionRequestHandlerGlobalCheck",
    "db:clean": "node dev-scripts/cli.js --delete",
    "dev:desktop": "cross-env NODE_ENV=development electron .",
    "dev:backend": "export DEBUG=app* && yarn dev:backend-start",
    "dev:backend -verbose": "export DEBUG=* && yarn dev:backend-start",
    "dev:backend-start": "cd packages/backend && ts-node-dev --respawn --pretty --transpile-only -r tsconfig-paths/register src/app.ts",
    "dev:web": "export DEV=true DEBUG=app* && cd packages/web && webpack serve --env development --mode=development",
    "dist:backend": "cd packages/backend && rm -r dist/ && tsc",
    "dist:desktop": " cd packages/electron && electron-builder --dir",
    "dist:desktop -a": "yarn test && yarn prod:build && electron-builder --mac --linux --windows",
    "dist:desktop -l": "yarn test && yarn prod:build && electron-builder --linux",
    "dist:desktop -m": "yarn test && yarn prod:build && electron-builder --mac",
    "dist:desktop -w": "yarn test && yarn prod:build && electron-builder --windows",
    "dist:web": "cd packages/web && webpack --env production --mode=production",
    "lint -f": "cross-env yarn workspaces run lint-fix",
    "lint:css": "stylelint 'packages/web/src/**/*.ts'",
    "postinstall": "husky install",
    "start:backend": "cd packages/backend && ts-node -r tsconfig-paths/register ./dist/backend/src/app.js",
    "start:desktop": "yarn prod:build && electron .",
    "start:web": "cross-env yarn workspace @compass/web start",
    "test:backend": "cd packages/backend && jest",
    "test:web": "echo *TODO update this after movin jest config to root* && yarn jest",
    "translate:desktop": "node packages/electron/localization/translateMissing.js"
  },
  "dependencies": {
    "@compass/backend": "*",
    "@compass/core": "*",
    "@compass/web": "*",
    "@types/uuid": "^8.3.1",
    "focus-trap-react": "^8.7.0",
    "inquirer": "^8.1.2",
    "jsonwebtoken": "^8.5.1",
    "lerna": "^4.0.0",
    "lodash": "4.17.21",
    "lodash.merge": "^4.6.2",
    "process": "^0.11.10",
    "source-map-support": "^0.5.20",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "babel-loader": "^8.2.2",
    "buffer": "^6.0.3",
    "commander": "^8.1.0",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.5",
    "path-browserify": "^1.0.1",
    "stream-browserify": "^3.0.0",
    "typescript": "^4.4.3"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": []
  }
}
